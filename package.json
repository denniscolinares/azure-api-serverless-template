    "jest": "^24.8.0",
    "jest-stare": "^1.27.0",
    "sequelize-cli": "^5.5.1",
    "serverless-pseudo-parameters": "^2.5.0"
  },
  "jest-stare": {
    "resultDir": "reports/latest",
    "coverageLink": "./coverage/index.html",
    "additionalResultsProcessors": []
  }
