  "license": "MIT",
  "dependencies": {
    "accounting": "git+https://github.com/openexchangerates/accounting.js.git",
    "app-root-path": "^3.0.0",
    "async": "^3.2.0",
    "axios": "^0.19.2",
    "azure-function-express": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "buffer-from": "^1.1.1",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "dynamoose": "^2.7.3",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
