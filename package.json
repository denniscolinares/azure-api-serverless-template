{
  "name": "azure-api-serverless-express-template",
  "private": true,
  "version": "1.0.0",
  "description": "API serverless template",
  "main": "index.js",
  "scripts": {
    "devStart": "serverless offline build && serverless offline start",
    "azDeployDev": "serverless deploy --stage dev",
    "azDeployTest": "serverless deploy --stage test",
    "azDeployStaging": "serverless deploy --stage stg",
    "azDeployProd": "serverless deploy --stage prod",
    "dbMigrate": "export NODE_ENV=dev | sequelize db:migrate",
    "test-coverage": "NODE_ENV=test ./node_modules/.bin/jest . --config=jest.config.json",
    "test": "jest .",
    "azureLogin": "az login --use-device-code"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "_moduleAliases": {
    "~": "./api"
  },
  "keywords": [],
